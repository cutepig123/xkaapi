NQUEENS_PRGS=nqueens_apiatha nqueens_apikaapi
check_PROGRAMS+=$(NQUEENS_PRGS)
all-nqueens: $(NQUEENS_PRGS)

#Nqueens program.
nqueens_apiatha_SOURCES=nqueens/nqueens_apiatha.cpp
