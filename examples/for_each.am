for_each_PRGS=for_each_0 for_each_1 for_each_rec_xx \
        for_each_0xx for_each_1xx for_each_2xx
if KAAPI_CXX_HAS_LAMBDA
for_each_PRGS+=for_each_0xx_lambda for_each_1xx_lambda
endif


check_PROGRAMS+=$(for_each_PRGS)

#For each variation
all-for_each: $(for_each_PRGS)
for_each_0_SOURCES=for_each/for_each_0.c
for_each_0_PKGLIBS=xkaapi
for_each_1_SOURCES=for_each/for_each_1.c
for_each_1_PKGLIBS=xkaapi
for_each_rec_xx_SOURCES=for_each/for_each_rec_xx.cpp
