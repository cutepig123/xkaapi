FOREACH_PRGS=for_each_0_kaapi for_each_1_kaapi for_each_2_kaapi
if KAAPI_API_KAAPIXX
FOREACH_PRGS+=\
	for_each_rec_kaapi++  for_each_0_kaapi++ for_each_1_kaapi++ \
	for_each_2_kaapi++
endif
if KAAPI_CXX_HAS_LAMBDA
FOREACH_PRGS+=for_each_0_kaapi++_lambda
endif

check_PROGRAMS+=$(FOREACH_PRGS)
all-for_each: $(FOREACH_PRGS)

#For each variation
for_each_0_kaapi_SOURCES=for_each/for_each_0_kaapi.c
for_each_1_kaapi_SOURCES=for_each/for_each_1_kaapi.c
for_each_2_kaapi_SOURCES=for_each/for_each_2_kaapi.c
